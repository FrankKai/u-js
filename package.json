{
  "name": "ujf",
  "version": "0.0.14",
  "description": "Utils for frontend or nodejs developers, fullname is Useful JS Functions",
  "main": "dist/index.js",
  "scripts": {
    "main-compile": "babel ujf.js --presets env -o ujf.common.js",
    "test-compile": "babel test --presets env -d test-compile",
    "src-compile": "babel src --presets env -d ./src-compile",
    "babel-node-compile": "babel 3.5.js --presets airbnb -o ./test/airbnb-js-style-guide/3.5.js",
    "parcel-build:node": "parcel ./src/index.js --target node --out-dir adapters/node",
    "parcel-build:browser": "parcel ./src/index.js --target browser --out-dir adapters/browser",
    "parcel-build:umd": "parcel serve ./adapters/index.js --global UJF --out-dir dist",
    "publish:patch": "npm version patch && npm publish --registry http://registry.npmjs.org && git push",
    "publish:minor": "npm version minor && npm publish --registry http://registry.npmjs.org && git push",
    "publish:major": "npm version major && npm publish --registry http://registry.npmjs.org && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FrankKai/UJF.git"
  },
  "keywords": [
    "javascript"
  ],
  "author": "FrankKai",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FrankKai/UJF/issues"
  },
  "homepage": "https://github.com/FrankKai/UJF#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.6.1"
  },
  "dependencies": {
    "audio-loader": "^1.0.3"
  }
}
